{
    "vercel": 2,
    "builds": [
        {
            "src": "./server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        { "src": "/api/products/(.*)", "dest": "/api/products/$1" },
        { "src": "/api/users/(.*)", "dest": "/api/users/$1" },
        { "src": "/auth/(.*)", "dest": "/auth/$1" },
        { "src": "/dashboard", "dest": "/dashboard" },
        { "src": "/logout", "dest": "/logout" },
        { "src": "/refresh", "dest": "/refresh" },
        { "src": "/(.*)", "dest": "/" }
    ]
}